{
    "name": "dms/web.laravel",
    "description": "The integration package with the laravel framework",
    "authors": [
        {
            "name": "Elliot Levin",
            "email": "elliotlevin@hotmail.com"
        }
    ],
    "minimum-stability": "stable",
    "repositories": [
        { "type": "composer", "url": "http://satis.dev:4321/" }
    ],
    "autoload": {
        "psr-4": {
            "Dms\\Web\\Laravel\\": "./src/"
        }
    },
    "require": {
        "php": ">=7.0.0",
        "dms/core": "dev-master",
        "dms/common.structure": "dev-master",
        "laravel/framework": "5.2.*",
        "monii/container-interop-laravel": "^1.0",
        "marcj/topsort": "^1.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Dms\\Web\\Laravel\\Tests\\": "./tests/Tests/"
        }
    },
    "require-dev": {
        "dms/common.testing": "dev-master",
        "orchestra/testbench": "~3.0"
    }
}
